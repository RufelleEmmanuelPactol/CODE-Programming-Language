


BEGIN CODE
    DISPLAY: "Enter your number: "
    INT num
    SCAN: num
    FOR (INT i=0; i<num; i = i+1)
    BEGIN FOR
        IF(NOT(i % 11))
            BEGIN IF
                DISPLAY: "I am a wonderkind, a special number: [i]!" & $
                CONTINUE
            END IF
        DISPLAY: "Hello, this is me, the number [i]!" & $
    END FOR
END CODE